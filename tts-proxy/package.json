{
  "name": "openvoiceproxy",
  "version": "1.0.0",
  "description": "OpenVoiceProxy - Secure TTS proxy that intercepts ElevenLabs API calls and redirects to various TTS engines",
  "main": "src/main.js",
  "scripts": {
    "start": "electron .",
    "start:production": "node server-production.js",
    "start:server": "node start-server.js",
    "dev": "electron . --dev",
    "test": "node src/test-proxy.js",
    "lint": "eslint src",
    "build:admin": "cd admin-ui && npm run build",
    "build:cli": "pkg cli/CallTTS.js -t node18-win-x64 -o dist/CallTTS.exe",
    "build:electron": "electron-builder --win",
    "build:all": "npm run build:admin && npm run build:cli && npm run build:electron",
    "postinstall": "cd admin-ui && npm install"
  },
  "keywords": [
    "tts",
    "proxy",
    "openvoiceproxy",
    "elevenlabs",
    "azure",
    "openai",
    "aws-polly",
    "google-tts"
  ],
  "author": "Will Wade",
  "license": "MIT",
  "devDependencies": {
    "@eslint/js": "^9.39.1",
    "electron": "^28.0.0",
    "electron-builder": "^24.13.3",
    "eslint": "^9.39.1",
    "globals": "^16.5.0",
    "pkg": "^5.8.1"
  },
  "dependencies": {
    "@aws-sdk/client-polly": "^3.821.0",
    "@google-cloud/text-to-speech": "^6.1.0",
    "axios": "^1.13.2",
    "commander": "^12.1.0",
    "cors": "^2.8.5",
    "decompress": "^4.2.1",
    "decompress-bzip2": "^4.0.0",
    "decompress-tarbz2": "^4.1.1",
    "decompress-targz": "^4.1.1",
    "dotenv": "^16.5.0",
    "express": "^4.18.2",
    "js-tts-wrapper": "^0.1.63",
    "mespeak": "^2.0.2",
    "microsoft-cognitiveservices-speech-sdk": "^1.44.0",
    "node-fetch": "^2.7.0",
    "node-wav": "^0.0.2",
    "openai": "^4.104.0",
    "pcm-convert": "^1.6.5",
    "pg": "^8.11.3",
    "sherpa-onnx-node": "^1.12.0",
    "tar-stream": "^3.1.7",
    "text2wav": "^0.0.14",
    "winston": "^3.11.0",
    "ws": "^8.18.3"
  },
  "optionalDependencies": {
    "sound-play": "^1.1.0",
    "speaker": "^0.5.5"
  }
}
